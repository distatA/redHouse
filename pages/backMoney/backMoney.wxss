.title{
  font-size:20px;
  font-family:PingFangSC;
  font-weight:400;
  color:rgba(0,0,0,1);
  margin-top: 10px;
  text-align: center;
  margin-bottom:15px;
}
.box{
  width:100%;
  height:20px;
  background: rgb(248, 246, 246);
  /* margin-top:15px; */
}
.list{
  padding:11px 20px;
  border-bottom:1px solid #F5F5F5;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size:12px;
  font-family:PingFangSC;
  font-weight:400;
  color:rgba(113,113,113,1);
}

.list image{
  width:26px;
  height:26px;
  border-radius: 50%;
  background: #abcdef;
  margin-right:8px;
}
.list .right{
  display: flex;
  align-items: center;
}
