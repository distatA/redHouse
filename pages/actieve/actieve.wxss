/* pages/actieve/actieve.wxss */
.user{
  width:100%;
  height:100%;
  background: rgba(68, 68, 68, 0.9);
  position: fixed;
  z-index: 99;
  top: 0;
  left: 0;
  display: flex;
  align-items: center; 
}
.user image{
  width:290px;
  height:40px;
  position: relative;
  margin:0 auto;
  display:block;
}
.btn-sets{
  position: absolute;
  width:100%;
  height:100%;
  top:0;
  left:0;
}
/* 轮播图 */
.u-wrp-bnr {
  width: 100%;
  height: 460rpx;
  display: block;
  position: relative;
  margin: 0 auto;
}
.u-img-slide {
  width: 100%;
  height: inherit;
  border-radius: 10px;
  background: #abcdef;
}
.main{
  background: #FF6B09;
  padding:10px;
}
/* 背景图 */
.topImg{
  width:100%;
  height:544px;
  background: #abcdef;
}
.topImg image{
  width:100%;
  height:100%;
}
/* 特价优惠 */
.title{
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding:13px 24px 10px 12px;
  box-sizing: border-box;
  font-size:15px;
  font-family:PingFangSC-Regular;
  font-weight:600;
  color:rgba(12,12,12,1);
  background: #fff;
  border-radius:5px 5px 0 0;
}
.title .left{display: flex;align-items: center}
.title .left text{
  display: block;
  width:2px;
  height:15px;
  background:linear-gradient(192deg,rgba(255,80,83,1) 0%,rgba(255,146,84,1) 100%);
  margin-right:5px;
}
.list{
  padding: 10px;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  margin-bottom:10px;
  background: #fff;
  border-radius:0 0 5px 5px;
}
.list .list-img{
  width:160px;
  height:147px;
  margin-bottom:14px;
  background: url(https://firsthouse.oss-cn-shanghai.aliyuncs.com/blog/2020-03-16/e8c9e02d20394c88a28e5859c63633e0-tmp_322409f1f554a51b38687e0c6add368c.jpg) center/100%;
  background-repeat:no-repeat;
}
.list-img .image{
  width:100%;
  height:100%;
  
}
.name{
  width:140px;
  margin:0 auto;
  font-size:14px;
  font-family:PingFangSC-Regular;
  font-weight:600;
  color:rgba(12,12,12,1);
  text-align: center;
  margin-top:20px;
  
  white-space:nowrap;
  text-overflow:ellipsis;
  overflow: hidden;
}
.place{
  width:140px;
  margin:0 auto;
  font-size:12px;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(12,12,12,1);
  text-align: center;
  margin-top:7px;
  white-space:nowrap;
  text-overflow:ellipsis;
  overflow: hidden;
}
.money{
  font-size:12px;
  font-family:PingFangSC-Regular;
  font-weight:550;
  color:rgba(255,255,255,1);
  text-align: center;
  margin-top:28px;
  white-space:nowrap;
  text-overflow:ellipsis;
  overflow: hidden;
}
button::after {
  border: none;
  background: none;
}
button[plain]{
  border:0;
}
