/* pages/hDetail/hDetail.wxss */
page{
  background: #F2F2F2;
}
.head{
  background: #FF7054;
  padding: 50rpx 40rpx;
  display: flex;
  justify-content: space-between;
}
.head .status{
  font-size:18px;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(255,255,255,1);
  line-height:25px;
}
.head .time{
  font-size:14px;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(255,255,255,1);
  line-height:20px;
  margin-top:10px;
}
.head .right{
  width:130rpx;
  height:124rpx;
}
.head .right image{
  width:100%;
  height:100%;
}
/* 房产 */
.list{
  padding:16px 20px 20px 20px;
  background: #fff;
  box-sizing: border-box;
  margin:10rpx 0;
}
.top{
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.top .left{
  display: flex;
  align-items: center;
}
.top .left .headUrl{
  width:26px;
  height:26px;
}
.top .left .headUrl image{
  width:100%;
  height:100%;
  border-radius:50%;
}
.shopName{
  display: flex;
  align-items: center;
  font-size:16px;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:550;
  color:rgba(0,0,0,1);
}
.shopName text{
  color:#9F9F9F;
  font-size: 12px;
  margin-left:5px;
}
.code{
  font-size:12px;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(120,120,120,1);
}
/* 优惠券 */
.juan{
  width:307px;
  height:80px;
  background: url(https://firsthouse.oss-cn-shanghai.aliyuncs.com/blog/2020-03-06/68c12c72179d48c19a30456caadb5ec4-tmp_fda5d4f45f8a3a2f145bc2d9e9a8746f.jpg) center/100%;
  background-repeat:no-repeat;
  padding-top:15px;
  margin:0 auto;
  margin-top:15px;
  box-sizing: border-box;
}
.juan-name{
  font-size:19px;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(255,255,255,1);
  text-align: center;
  padding-right:48px;
  box-sizing: border-box;
  overflow: hidden;
text-overflow:ellipsis;
white-space: nowrap;

}
.juan-time{
  font-size:8px;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(255,255,255,1);
  text-align: center;
  margin-top:8px;
  padding-right:48px;
  box-sizing: border-box;
}
/* 推荐有奖 */
.money{
  height:22px;
  padding:3px 20px 3px 10px;
  background: linear-gradient(to right,#FB9356,#FE5150);
  border-radius:10px 0 0 10px;
  font-size:15px;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(255,255,255,1);
}
.tip{
  padding:20px 20px 10px 20px;
  font-size:14px;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(0,0,0,1);
  line-height: 23px;
}
/* 房产信息 */
.bottom{
  display: flex;
  align-items: center;
  margin-top:22px;
}
.houseImg{
  width:80px;
  height:80px;
}
.houseImg image{
  width:100%;
  height:100%;
}
.deteil{
  margin-left:12px;
  width:70%;
}
.huoseName{
  font-size:16px;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:#000;
  line-height:20px;
}
.houhui{
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size:12px;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(255,112,84,1);
  line-height:17px;
  margin-top:13px;
}
.place{
  font-size:12px;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(113,113,113,1);
  line-height:17px;
  margin-top:10px;
}
/* 预约信息 */
.list .title{
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-left:2px solid #FF7254;
  font-size:16px;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:550;
  color:rgba(0,0,0,1);
  line-height:20px;
  padding:0 8px;
}
.list .li{
  font-size:14px;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:#6F6F6F;
  line-height:17px;
  margin-top:7px;
  padding:0 23px;
  display: flex;
  align-items: flex-start;
}
/* to偶素 */
.news{
  margin-top:18px;
  display: flex;
  align-items: flex-start;
}
.news .right{
  margin-left:10px;
  width:315px;
}
.news .headImg{
  width:40px;
  height:40px;
}
.news .headImg image{
  width:100%;
  height:100%;
  border-radius:50%;
}
.news .name{
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size:12px;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(117,117,117,1);
  line-height:17px;
}
.news .contain{
  font-size:12px;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(0,0,0,1);
  line-height:17px;
  margin-top:10px;
}
/* 投诉 */
.imgs{
  margin-top:5px;
  display: flex;
  align-items: center;
  flex-wrap: wrap;
}
.imgs image{
  width:90px;
  height:90px;
  background: #abcdef;
  border-radius:5px;
  margin-right:6px;
  margin-bottom:6px;
}
/* 底部 */
.foot{
  width:100%;
  height:50px;
  padding:13px 20px;
  display: flex;
  align-items: center;
  justify-content: flex-end;
  box-sizing: border-box;
  background: #fff;
}
.foot .pingjia{
  width:60px;
  height:24px;
  border-radius:5px;
  border:1px solid rgba(255,108,110,1);
  line-height: 24px;
  text-align: center;
  font-size:14px;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(255,80,83,1);
}
.foot .tousu{
  width:60px;
  height:24px;
  border-radius:5px;
  border:1px solid rgba(130,127,127,1);
  line-height: 24px;
  text-align: center;
  font-size:14px;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(130,127,127,1);
  margin-left:16px;
}
/* 核销 */
.main{
  width:100%;
  /* height:400px; */
  /* padding:15px; */
  box-sizing: border-box;
  position: relative;
  padding-bottom:40px;
}
.boxTop{
  width:100%;
  height:80px;
  background: url(https://firsthouse.oss-cn-shanghai.aliyuncs.com/blog/2020-03-09/afd930f746c64006b15172ac5e835711-tmp_a843e621c02d32a920ec55a5cc22a402.jpg) center/100%;
  background-repeat:no-repeat;
  font-size:22px;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(255,255,255,1);
  /* line-height:240rpx; */
  text-align: center;
  border-radius:12px 12px 0 0;
  padding-top: 25px;
  box-sizing: border-box;
}

.clos{
  position:absolute;
  top:27rpx;
  right:27rpx;
  width:20px;
  height:20px;
}
.main image{
 width:264rpx;
 height:300rpx;
 margin:0 auto;
 display: block;
}
.main .code{
  font-size:15px;
  font-family:PingFangSC-Regular,PingFangSC;
  font-weight:400;
  color:rgba(255,146,84,1);
  margin:20px 0;
  text-align: center;
}
.van-center-enter-active,.van-center-leave-active{
  /* width:90%; */
  /* height:75%; */
  border-radius:12px;
  
}
.main .num{
  font-size:14px;
  font-family:PingFangSC-Regular,PingFangSC;
  font-weight:400;
  color:#000;
  text-align: center;
  margin-top:20px;
}
.main .title{
  font-size:14px;
  font-family:PingFangSC-Regular,PingFangSC;
  font-weight:400;
  color:#000;
  margin-top:20px;
  margin-left:10%;
   margin-right:10px;
  white-space:nowrap;
   overflow:hidden;
   text-overflow:ellipsis;
}

/* 弹框 */
.zz{
	position:fixed;
  width:100%;
  height:100%;
	left:0;
	top:0;
	background:rgba(0,0,0,0.7);
	z-index:999;
	display:flex;
	align-items:center;
}
.zz-box{
  /* width:100%; */
 background:rgba(255,255,255,1);
 border-radius:15px 15px 10px 10px;
 margin: 0 auto;
 position: relative;
}
.zz-box .title{
  font-size:45rpx;
text-align: center;
font-weight:bold;
margin-top:109rpx;
height:42rpx;
line-height: 42rpx;
color:rgba(4,7,23,1);
}
.zz.no{
	display:none;
}
.list-juan{
  width:295px;
  height:77px;
  background: url(https://firsthouse.oss-cn-shanghai.aliyuncs.com/blog/2020-03-09/b439dc8939f945ce8aa6a0b157384a68-tmp_0f477f79a17488a5b374f62474657af4.jpg) center/100%;
  background-repeat:no-repeat;
  margin:7px auto;
  padding-top:15px;
  box-sizing: border-box;
}
.checked{
  width:295px;
  height:77px;
  background: url(https://firsthouse.oss-cn-shanghai.aliyuncs.com/blog/2020-03-09/f3e9dcc9969242819ac2639696dfa3d2-tmp_1584eb580672c3898ed3ceb233dccfcf.jpg) center/100%;
  background-repeat:no-repeat;
  margin:10px auto;
  padding-top:15px;
  box-sizing: border-box;
}
