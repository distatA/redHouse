<!--pages/kol/kol.wxml-->
<!-- <view class="clickMask" wx:if="{{showClickMask}}" bindtap="clickMask">
</view> -->
  
<!-- 打赏 -->
<view class="zz {{display == 1 ?'no':''}}" catchtouchmove="myCatchTouch">
  <view class="zz-box" style="width:310px;">
    <view class="clos" bindtap="closeZz">
      <image src='/img/icon/cha.png' style="width:20px;height:20px;display:block;margin:0"></image>
    </view>
    <view class="box-title">我要打赏</view>
    <image src="{{myheadUrl}}" style="width:120rpx;height:120rpx;border-radius:50%;margin:10px auto;display:block"></image>
    <view class="box-name">{{mynickName}}</view>
    <block wx:if="{{!otherTrue}}">
      <view class="box-item">
        <view class="box-list" wx:for="{{moneyist}}" wx:key="index" bindtap="pay" id="{{item}}" data-type="1">{{item}}元</view>
      </view>
      <view class="other" bindtap="other">其他金额</view>
    </block>
    <block wx:else>
      <view class="custom">
        <input type="digit" style="text-align:center" bindinput="getValue" value="{{moneyValue}}"></input>
        <text style="color:#FF7054">元</text>
      </view>
      <view class="other" style="margin:20px 0;" bindtap="back">固定金额</view>
      <view class="box-foot" bindtap="pay" data-type="2">确定</view>
    </block>
  </view>
</view>
<view class="tempKol">
  <!-- 顶部 -->
  <view class="top">
    <!-- 搜索 -->
    <view class="search">
      <input type="text" class="topsearch" placeholder="请输入您要搜索的内容" bindinput="getInputValue" bindconfirm='searchHandle' value="{{searchValue}}"></input>
      <view class="searchPic" bindtap="toSearch"></view>
    </view>
    <!-- <image src="../../img/icon/car.png" style="width:20px;height:18px" bindtap="cartAccess"></image> -->
  </view>
  <view class="tabSlide">
    <view class="tab">
      <block wx:for="{{hrList}}" wx:key="index">
        <view class="tab-item {{currentTab == index ? 'active' : ''}}" data-current="{{index}}" bindtap='clickMenu' id="{{item.num}}">
          <!-- <view class="star"></view> -->
          <text>{{item.name}}</text>
        </view>
      </block>
    </view>
    <!-- 推荐 -->
    <view wx:if="{{currentTab==0}}">
      <view style="width:100%;height:6px; opacity:0;"></view>
      <swiper class='u-wrp-bnr' indicator-dots='true' autoplay='true' interval='5000' duration='1000' circular='true' indicator-color='#c1c1c1' indicator-active-color='#ff6666' wx:if="{{bnrUrl.length>0}}">
        <block wx:for="{{bnrUrl}}" wx:for-index="index" wx:key="key">
          <swiper-item>
            <image src='{{item.imgUrl}}' class='u-img-slide' mode='aspectFill' style="border-radius:10px" bindtap='viewBanner' id="{{item.type}}" data-id="{{item.appUrl}}"></image>
          </swiper-item>
        </block>
      </swiper>

      <view class="allList">
        <!-- 笔记 -->
        <view wx:for="{{recommendList}}" wx:key="index" wx:if="{{item.type==1||item.type==3||item.type==5||item.type==6||item.type==7}}">
          <view class="liContent">
            <view class="liTop">
              <view style="display: flex;align-items: center">
                <image src="{{item.headUrl}}" class="avarImg" style="width: 80rpx;height: 80rpx;border-radius:50%;"></image>
                <view class="liAvar" bindtap="redHome" id="{{item.member_no}}">
                  <view style="display: flex;align-items: center">
                    <view class="nickName {{item.nickName.length>20?'nickNames':''}}">{{item.nickName}}</view>
                    <view class="vipLevel" wx:if="{{item.redType==1||item.redType==2}}">
                      <image src="/img/icon/vip.png" role="img"></image>
                    </view>
                  </view>
                  <view class="time">{{item.create_time}}</view>
                </view>
              </view>
              <view>
                <!-- 打赏人数 -->
                 <view class="people" wx:if="{{item.rewardList.length>0}}" bindtap="browseMore" id="{{item.member_no}}" data-id="{{item.id}}" data-reward="{{item.rewardList}}" data-name="{{item.nickName}}" data-head="{{item.headUrl}}">
                  <view class="browseImgItem">
                    <block class="houseIteam" wx:for="{{item.rewardList}}" wx:key="index">
                      <view class="browseImgView" hover-class="none" hover-stop-propagation="false">
                        <image src="{{item.headUrl}}" class="browseImg" />
                      </view>
                    </block>
                  </view>
                  <view class="left">
                    <text style="font-weight:520;">{{item.rewardList.length}}</text>人打赏</view>
                </view>
              <!--  <image src="/img/icon/shang.png" wx:if="{{item.rewardList.length==0}}" style="width:52rpx;height:52rpx;"                            bindtap="shang" id="{{item.id}}" data-name="{{item.nickName}}" data-head="{{item.headUrl}}"></image> -->
              </view>
            </view>
            <!-- 内容 -->
            <view class="main">
              <view class="liTitle" bindtap="noteDetail" id="{{item.id}}" data-type="{{item.type}}" data-comment="{{item.comment_num}}" data-time="{{item.create_time}}" data-headUrl="{{item.headUrl}}" data-nickName="{{item.nickName}}" data-imgs="{{item.memberNoteArticleImageList}}"
                data-member="{{item.member_no}}" data-title="{{item.title}}" data-video="{{item.videoUrls}}" data-like="{{item.like_num}}" data-read="{{item.read_num}}" data-flag="{{item.flag}}" data-spu="{{item.spu_no}}" data-goods_name="{{item.spu_name}}"
                data-member_no="{{item.member_no}}" data-choice="{{item.category_choice}}">
                <view wx:if="{{item.type==5||item.type==6}}">
                  <image src="/img/icon/xun.png" style="width:40px;height:14px;margin-right:15px"></image>
                </view> {{item.type==3?item.subtitle:item.title}}</view>
              <!-- 快讯内容 -->
              <view class="kuaixun" wx:if="{{item.type==5||item.type==6}}" bindtap="noteDetail" id="{{item.id}}" data-type="{{item.type}}" data-comment="{{item.comment_num}}" data-time="{{item.create_time}}" data-headUrl="{{item.headUrl}}" data-nickName="{{item.nickName}}"
                data-imgs="{{item.memberNoteArticleImageList}}" data-member="{{item.member_no}}" data-title="{{item.title}}" data-video="{{item.videoUrls}}" data-like="{{item.like_num}}" data-read="{{item.read_num}}" data-flag="{{item.flag}}" data-spu="{{item.spu_no}}"
                data-goods_name="{{item.spu_name}}" data-member_no="{{item.member_no}}" data-choice="{{item.category_choice}}">{{item.memberNoteArticle.content}}</view>
              <!-- 笔记图片 -->
              <view class="showPics" style="flex-wrap:wrap" wx:if="{{item.type==1}}">
                <block wx:if="{{item.memberNoteArticleImageList.length==1}}">
                  <image wx:for="{{item.memberNoteArticleImageList}}" wx:for-item="its" wx:key="key" src="{{its.imageUrl}}" bindtap="clickImg" id="{{index}}" data-list="{{item.memberNoteArticleImageList}}" mode="widthFix" style="width:420rpx" class="pics"></image>
                </block>
                <block wx:if="{{item.memberNoteArticleImageList.length>2}}">
                  <image wx:for="{{item.memberNoteArticleImageList}}" wx:for-item="its" wx:key="key" src="{{its.imageUrl}}" bindtap="clickImg" id="{{index}}" data-list="{{item.memberNoteArticleImageList}}" mode="aspectFill"></image>
                </block>
              </view>
              <view class="showPics" style="justify-content:flex-start" wx:if="{{item.memberNoteArticleImageList.length==2}}">
                <image class="pics" wx:for="{{item.memberNoteArticleImageList}}" wx:for-item="its" wx:key="key" src="{{its.imageUrl}}" bindtap="clickImg" id="{{index}}" data-list="{{item.memberNoteArticleImageList}}" style="margin-left:12rpx;" mode="aspectFill"></image>
              </view>
              <!-- 后台发布的文章 -->
              <view class="showPics" style="position: relative;" wx:if="{{item.type==3}}">
                <image src="{{item.memberNoteArticleImageList[0].imageUrl}}" bindtap="noteDetail" id="{{item.id}}" data-type="{{item.type}}" data-comment="{{item.comment_num}}" data-time="{{item.create_time}}" data-headUrl="{{item.headUrl}}" data-nickName="{{item.nickName}}"
                  data-imgs="{{item.memberNoteArticleImageList}}" data-member="{{item.member_no}}" data-title="{{item.title}}" data-video="{{item.videoUrls}}" data-like="{{item.like_num}}" data-read="{{item.read_num}}" data-flag="{{item.flag}}" data-spu="{{item.spu_no}}"
                  data-goods_name="{{item.spu_name}}" data-member_no="{{item.member_no}}" data-choice="{{item.category_choice}}" mode="widthFix" style="width:100%;margin:0;" class="pics"></image>
                <view class="boxFei" wx:if="{{item.isPay==1}}">付费</view>
                <view class="boxSon">{{item.title}}</view>
              </view>
              <!-- 视频 -->
              <block wx:if="{{item.videoUrls}}">
                <video src="{{item.videoUrls}}" id="houseVideo" title="{{item.title}}" bindfullscreenchange="fullscreenchange" mode="widthFix">
                </video>
              </block>
              <!-- 投票 -->
              <view class="vote" wx:if="{{item.memberArtVote.title}}">
                <view class="vote-top">
                  <text style="margin-right:4px;">|</text>投票</view>
                <view class="vote-title">{{item.memberArtVote.title}}</view>
                <!-- 文字 -->
                <block wx:if="{{item.memberArtVote.type==1}}">
                  <!-- 未开始 -->
                  <block wx:if="{{item.isVote==0}}">
                    <view class="vote-list" wx:for="{{item.memberArtVote.list}}" wx:key="key" bindtap="onclicks1">
                      <view class="left">{{item.title}}</view>
                      <view class="right" wx:if="{{item.checked}}">{{item.num}}</view>
                    </view>
                  </block>
                  <!-- 进行中 -->
                  <block wx:if="{{item.isVote==1}}">
                    <view class="vote-list" wx:for="{{item.memberArtVote.list}}" wx:key="key" bindtap="onclicks" id="{{it.id}}" style="padding:0 {{it.checked==0?'15px':'0px'}}" data-target="{{it.target}}" data-chose="{{it.chose}}" data-item="{{item.memberArtVote.list}}"
                      wx:for-item="it" data-pid="{{item.id}}">
                      <block wx:if="{{it.checked}}">
                        <view class="left" style="background:{{it.target==1?'#FFE8E5':'#E7E7E8'}};padding:0 5px">{{it.title}}</view>
                      </block>
                      <block wx:if="{{!it.checked}}">
                        <view class="left">{{it.title}}</view>
                      </block>
                      <view class="right" wx:if="{{it.checked}}" style="color:{{it.target==1?'#FF6D53':''}}">{{it.num}}</view>
                    </view>

                  </block>
                  <!-- 已结束 -->
                  <block wx:if="{{item.isVote==2}}">
                    <view class="vote-list" wx:for="{{item.memberArtVote.list}}" wx:key="key" bindtap="onclickss" style="padding:0;box-sizing:border-box;" data-target="{{item.target}}">
                      <view class="left" style="background:#E7E7E8;padding:0 5px;">{{item.title}}</view>
                      <view class="right">{{item.num}}</view>
                    </view>
                  </block>
                </block>
                <!-- 图片 -->
                <block wx:if="{{item.memberArtVote.type==2}}">
                  <view class="voteImg">
                    <!-- 未开始 -->
                    <block wx:if="{{item.isVote==0}}">
                      <view class="img-item" wx:for="{{item.memberArtVote.list}}" wx:key="key">
                        <image src="{{item.image}}" class="img-items">
                          <view class="voteBox">《{{item.title}}》</view>
                        </image>
                        <view class="vote-piao" bindtap="onclicks1">投票</view>
                      </view>
                    </block>
                    <!-- 进行中 -->
                    <block wx:if="{{item.isVote==1}}">
                      <view class="img-item" wx:for="{{item.memberArtVote.list}}" wx:key="key" wx:for-item="it">
                        <image src="{{it.image}}" class="img-items">
                          <view class="voteBox">《{{it.title}}》</view>
                        </image>
                        <view class="vote-piao" bindtap="onclicks" id="{{it.id}}" data-target="{{it.target}}" data-chose="{{it.chose}}" data-item="{{item.memberArtVote.list}}" data-pid="{{item.id}}">投票</view>
                      </view>
                    </block>
                    <!-- 已结束 -->
                    <block wx:if="{{item.isVote==2}}">
                      <view class="img-item" wx:for="{{item.memberArtVote.list}}" wx:key="key">
                        <image src="{{item.image}}" class="img-items">
                          <view class="voteBox">《{{item.title}}》</view>
                        </image>
                        <view class="vote-piao" bindtap="onclickss">投票</view>
                      </view>
                    </block>
                  </view>
                  <view class="vote-tiao" wx:if="{{item.memberArtVote.list[0].checked||item.memberArtVote.list[1].checked}}">
                    <view class="tiao-left" style="width:{{item.memberArtVote.list[0].num/(item.memberArtVote.list[0].num+item.memberArtVote.list[1].num)*100}}%" wx:if="{{item.memberArtVote.list[0].num!=0}}">{{item.memberArtVote.list[0].num}}</view>
                    <view class="tiao-right" style="width:{{item.memberArtVote.list[1].num/(item.memberArtVote.list[0].num+item.memberArtVote.list[1].num)*100}}%" wx:if="{{item.memberArtVote.list[1].num!=0}}">{{item.memberArtVote.list[1].num}}</view>
                  </view>
                </block>
                <view class="vote-time">{{item.voteNum}}人参与
                  <text style="margin-left:15px;">{{item.voteText}}</text>
                </view>
              </view>
              <!-- 链接商品 -->
              <view class="lianPage" style="justify-content:{{item.spu_name?'space-between':'flex-end'}}">
                <view class="left" wx:if="{{item.spu_name}}" bindtap="shop" id="{{item.spu_no}}" data-id="{{item.type}}" data-goods_name="{{item.spu_name}}" data-member_no="{{item.member_no}}" data-choice="{{item.category_choice}}">
                  <image src="https://firsthouse.oss-cn-shanghai.aliyuncs.com/images/shequ/lianPic.png"></image>
                  <view class="lianTitle {{item.spu_name.length>10?'lianTitles':''}}">{{item.spu_name}}</view>
                </view>
                <!-- 右边 -->
                <view class="right">
                  <view class="share">
                    <image src="/img/icon/redShare.png" style="width:24rpx;height:28rpx;"></image>
                    <view style="position: relative;color:#E66F63;margin-left:10px">分享
                      <button class="btn-set" form-type="submit" open-type="share" plain="{{true}}" id="{{item.id}}" data-type="{{item.type}}" data-comment="{{item.comment_num}}" data-time="{{item.create_time}}" data-headUrl="{{item.headUrl}}" data-nickName="{{item.nickName}}"
                        data-member="{{item.member_no}}" data-title="{{item.title}}" data-like="{{item.like_num}}" data-read="{{item.read_num}}" data-flag="{{item.flag}}" data-video="{{item.videoUrls}}" data-spu="{{item.spu_no}}" data-goods_name="{{item.spu_name}}"
                        data-member_no="{{item.member_no}}" data-choice="{{item.category_choice}}" data-imgUrl="{{item.memberNoteArticleImageList[0].imageUrl}}"></button>
                    </view>
                  </view>
                  <view class="clickRight" bindtap="onclick" id="{{index}}">
                    <image src="/img/icon/clickRight.png" style="z-index:999"></image>
                  </view>
                  <!-- 点赞评论弹窗 -->
                  <view class="yinBox" wx:if="{{item.isView}}">
                    <view class="left" bindtap="noteDetail" id="{{item.id}}" data-type="{{item.type}}" data-comment="{{item.comment_num}}" data-time="{{item.create_time}}" data-headUrl="{{item.headUrl}}" data-nickName="{{item.nickName}}" data-member="{{item.member_no}}"
                      data-title="{{item.title}}" data-like="{{item.like_num}}" data-read="{{item.read_num}}" data-flag="{{item.flag}}" data-video="{{item.videoUrls}}" data-spu="{{item.spu_no}}" data-goods_name="{{item.spu_name}}" data-member_no="{{item.member_no}}"
                      data-choice="{{item.category_choice}}">
                      <image src="/img/icon/v-c.png" style="width:30rpx;height:30rpx;"></image>
                      <view style="margin-left:4px">评论</view>
                    </view>
                    <view class="xian"></view>

                    <view class="left" data-flag="{{item.flag}}" bindtap="zan" id="{{item.id}}" data-type="{{item.type}}" data-title="{{item.title}}" data-member="{{item.member_no}}">

                      <image src="/img/icon/v-zan.png" style="width:28rpx;height:26rpx;"></image>
                      <view style="margin-left:4px" wx:if="{{item.flag==0}}">喜欢</view>
                      <view style="margin-left:4px" wx:if="{{item.flag==1}}">取消</view>
                    </view>

                  </view>
                  <!--  -->
                </view>
              </view>
              <view style="margin:7px 0px;height:1px;background:#F8F8F8" wx:if="{{item.nickNames.length>0||item.memberNoteAndArticleComments.length>0}}"></view>
              <!-- 点赞人与评论列表 -->
              <view class="list-box" wx:if="{{item.nickNames.length>0||item.memberNoteAndArticleComments.length>0}}">
                <view class="v-zan" wx:if="{{item.nickNames.length>0}}">
                  <view class="left">
                    <image src="/img/icon/zan0.png" style="width:15px;height:14px;"></image>
                  </view>
                  <view class="zan-name" wx:for="{{item.nickNames}}" wx:key="index">{{item}}</view>
                </view>
                <view class="v-zan" style="margin-top:10px;">
                  <view class="left" wx:if="{{item.memberNoteAndArticleComments.length>0}}">
                    <image src="/img/icon/comment.png" style="width:15px;height:15px;"></image>
                  </view>
                  <view>
                    <view class="comList" wx:for="{{item.memberNoteAndArticleComments}}" wx:key="index">
                      <view class="com-name">{{item.nick_name}}：</view>
                      <view class="com-contain">{{item.context}}</view>
                    </view>
                  </view>
                </view>
              </view>
              <!--  -->
            </view>
          </view>
          <view class="bottom" style=" border-bottom:6px solid #F9F9F9;"></view>
        </view>
        <!-- 外链文章 -->
        <view wx:for="{{recommendList}}" wx:key="index" wx:if="{{item.type==4}}">
          <view class="liContent">
            <!-- 多张图片 -->
            <view class="showListType" wx:if="{{item.memberNoteArticleImageList.length > 1}}">
              <view class="liTop">
                <view style="display: flex;align-items: center">
                  <image src="{{item.headUrl}}" class="avarImg" style="width: 80rpx;height: 80rpx;border-radius:50%;"></image>
                  <view class="liAvar" bindtap="redHome" id="{{item.member_no}}">

                    <view style="display: flex;align-items: center">
                      <view class="nickName {{item.nickName.length>20?'nickNames':''}}">{{item.nickName}}</view>
                      <view class="vipLevel" wx:if="{{item.redType==1||item.redType==2}}">
                        <image src="/img/icon/vip.png" role="img"></image>
                      </view>
                    </view>
                    <view class="time">{{item.create_time}}</view>
                  </view>
                </view>
                <view>
                  <!-- 围观人数 -->
                   <view class="people" wx:if="{{item.rewardList.length>0}}" bindtap="browseMore" id="{{item.member_no}}" data-id="{{item.id}}" data-reward="{{item.rewardList}}" data-name="{{item.nickName}}" data-head="{{item.headUrl}}">
                    <view class="browseImgItem">
                      <block class="houseIteam" wx:for="{{item.rewardList}}" wx:key="index">
                        <view class="browseImgView" hover-class="none" hover-stop-propagation="false">
                          <image src="{{item.headUrl}}" class="browseImg" />
                        </view>
                      </block>
                    </view>
                    <view class="left">
                      <text style="font-weight:520;">{{item.rewardList.length}}</text>人打赏</view>
                  </view>
                 <!-- <image src="/img/icon/shang.png" wx:if="{{item.rewardList.length==0}}" style="width:52rpx;height:52rpx;" bindtap="shang" id="{{item.id}}" data-name="{{item.nickName}}" data-head="{{item.headUrl}}"></image> -->
                </view>
              </view>
              <view class="tuwen">
                <view class="liTitle" bindtap="noteDetail" id="{{item.id}}" data-type="{{item.type}}" data-comment="{{item.comment_num}}" data-time="{{item.create_time}}" data-headUrl="{{item.headUrl}}" data-nickName="{{item.nickName}}" data-imgs="{{item.memberNoteArticleImageList}}"
                  data-member="{{item.member_no}}" data-title="{{item.title}}" data-video="{{item.videoUrls}}" data-like="{{item.like_num}}" data-read="{{item.read_num}}" data-flag="{{item.flag}}" data-spu="{{item.spu_no}}" data-goods_name="{{item.spu_name}}"
                  data-member_no="{{item.member_no}}" data-choice="{{item.category_choice}}">{{item.title}}</view>
                <!-- <block wx:if="{{item.memberNoteArticleImageList.length > 1}}"> -->
                <view class="showPics">
                  <image wx:for="{{item.memberNoteArticleImageList}}" wx:for-item="its" wx:key="key" src="{{its.imageUrl}}" bindtap="clickImg" id="{{index}}" data-list="{{item.memberNoteArticleImageList}}" class="pics"></image>
                </view>
                <!-- </block> -->
              </view>
              <!-- 链接商品 -->
              <view class="lianPage" style="justify-content:{{item.spu_name?'space-between':'flex-end'}}">
                <view class="left" wx:if="{{item.spu_name}}" bindtap="shop" id="{{item.spu_no}}" data-id="{{item.type}}" data-goods_name="{{item.spu_name}}" data-member_no="{{item.member_no}}" data-choice="{{item.category_choice}}">
                  <image src="https://firsthouse.oss-cn-shanghai.aliyuncs.com/images/shequ/lianPic.png"></image>
                  <view class="lianTitle {{item.spu_name.length>10?'lianTitles':''}}">{{item.spu_name}}</view>
                </view>
                <!-- 右边 -->
                <view class="right">
                  <view class="share">
                    <image src="/img/icon/redShare.png" style="width:24rpx;height:28rpx;"></image>
                    <view style="position: relative;color:#E66F63;margin-left:10px">分享
                      <button class="btn-set" form-type="submit" open-type="share" plain="{{true}}" id="{{item.id}}" data-type="{{item.type}}" data-comment="{{item.comment_num}}" data-time="{{item.create_time}}" data-headUrl="{{item.headUrl}}" data-nickName="{{item.nickName}}"
                        data-member="{{item.member_no}}" data-title="{{item.title}}" data-like="{{item.like_num}}" data-read="{{item.read_num}}" data-flag="{{item.flag}}" data-video="{{item.videoUrls}}" data-spu="{{item.spu_no}}" data-goods_name="{{item.spu_name}}"
                        data-member_no="{{item.member_no}}" data-choice="{{item.category_choice}}" data-imgUrl="{{item.memberNoteArticleImageList[0].imageUrl}}"></button>
                    </view>
                  </view>
                  <view class="clickRight" bindtap="onclick" id="{{index}}">
                    <image src="/img/icon/clickRight.png"></image>
                  </view>
                  <!-- 点赞评论弹窗 -->
                  <view class="yinBox" wx:if="{{item.isView}}">
                    <view class="left" bindtap="noteDetail" id="{{item.id}}" data-type="{{item.type}}" data-comment="{{item.comment_num}}" data-time="{{item.create_time}}" data-headUrl="{{item.headUrl}}" data-nickName="{{item.nickName}}" data-member="{{item.member_no}}"
                      data-title="{{item.title}}" data-like="{{item.like_num}}" data-read="{{item.read_num}}" data-flag="{{item.flag}}" data-video="{{item.videoUrls}}" data-spu="{{item.spu_no}}" data-goods_name="{{item.spu_name}}" data-member_no="{{item.member_no}}"
                      data-choice="{{item.category_choice}}">
                      <image src="/img/icon/v-c.png" style="width:30rpx;height:30rpx;"></image>
                      <view style="margin-left:4px">评论</view>
                    </view>
