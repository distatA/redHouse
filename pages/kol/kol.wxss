  width: 100%;
  height: 28px;
  background: #E7E7E8;
  border-radius: 4px;
  display: flex;
}
.tiao-left {
  width: 50%;
  height: 28px;
  line-height: 28px;
  background: #ffe8e5;
  border-radius: 4px 0px 0px 4px;
  font-size: 12px;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #ff6d53;
