/* pages/myCard/myCard.wxss */
.list{
  position: relative;
  width:100%;
  height:190px;
  margin:0 auto;
  padding:0 5px;
  box-sizing: border-box;
  margin-top:7px;
}
.list image{
  width:100%;
  height:100%;
  border-radius: 5px;
  /* background: #abcdef; */
}
.list .time{
  position: absolute;
  bottom:12px;
  left:11px;
  font-size:15px;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:600;
  color:rgba(255,255,255,1);
  margin-left:10px;
}